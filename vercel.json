{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "COOKIE_SECURE": "true",
      "COOKIE_SAMESITE": "none"
    }
  },
  "images": {
    "domains": [
      "vercel.app"
    ]
  },
  "cookies": {
    "sameSite": "none",
    "secure": true
  }
}