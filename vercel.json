{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Set-Cookie",
          "value": "__vercel_live_token=*; SameSite=None; Secure; Path=/"
        }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "COOKIE_SECURE": "true",
      "COOKIE_SAMESITE": "none"
    }
  },
  "images": {
    "domains": [
      "vercel.app"
    ],
    "sizes": [256, 512, 1024, 2048]
  }
}